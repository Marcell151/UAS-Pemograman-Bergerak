<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.kantinkampus">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" android:maxSdkVersion="32" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" />
    <uses-permission android:name="android.permission.READ_MEDIA_VISUAL_USER_SELECTED" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Base.Theme.UAS_PemrogramanBergerak"
        tools:targetApi="31">

        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginActivity"
            android:exported="false" /> <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ProfileActivity" android:label="Edit Profil" />
        <activity android:name=".SellerDashboardActivity" />
        <activity android:name=".ManageStandActivity" />
        <activity android:name=".ManageMenusActivity" />
        <activity android:name=".ManageOrdersActivity" />
        <activity android:name=".StandListActivity" />
        <activity android:name=".MenuListActivity" />
        <activity android:name=".MenuDetailActivity" />
        <activity android:name=".CartActivity" />
        <activity android:name=".OrderHistoryActivity" />
        <activity android:name=".FavoritesActivity" />
        <activity android:name=".AddReviewActivity" />
        <activity android:name=".AboutActivity" />

        <activity
            android:name=".OrderDetailActivity"
            android:label="Detail Pesanan"
            android:parentActivityName=".OrderHistoryActivity" /> </application>

</manifest>